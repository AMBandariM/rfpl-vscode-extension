{
  "name": "rfpl-language-support",
  "displayName": "RFPL Syntax Highlighter",
  "description": "This is an RFPL highlighter",
  "version": "0.0.1",
  "publisher": "rfpl-support",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "rfpl",
      "aliases": ["RFPL", "rfpl"],
      "extensions": [".rfpl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "rfpl",
      "scopeName": "text.rfpl",
      "path": "./syntaxes/rfpl.tmLanguage.json"
    }]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AMBandariM/rfpl-vscode-extension.git"
  }
}
