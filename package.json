{
  "name": "rfpl-language-support",
  "displayName": "RFPL Language Support",
  "description": "this is an RFPL highlighter",
  "version": "0.0.1",
  "publisher": "amir-mohammad",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "rfpl",
      "aliases": ["RFPL", "rfpl"],
      "extensions": [".rfpl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "rfpl",
      "scopeName": "text.rfpl",
      "path": "./syntaxes/rfpl.tmLanguage.json"
    }]
  }
}
